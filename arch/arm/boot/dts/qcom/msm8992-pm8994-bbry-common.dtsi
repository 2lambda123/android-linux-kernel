&pm8994_gpios {
	gpio@c100 { /* GPIO 2 */
		/* VOL_UP */
		qcom,mode = <0>;		/* Digital in */
		qcom,pull = <0>;		/* PULL up 30uA */
		qcom,vin-sel = <2>;		/* Logical 1 voltage value 1.8v */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};

	gpio@c200 { /* GPIO 3 */
		/* VOL_DOWN */
		qcom,mode = <0>;		/* Digital in */
		qcom,pull = <0>;		/* PULL up 30uA */
		qcom,vin-sel = <2>;		/* Logical 1 voltage value 1.8v */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};

	gpio@c300 { /* GPIO 4 */
		/* MUTE_KEY */
		qcom,mode = <0>;		/* Digital in */
		qcom,pull = <0>;		/* PULL up 30uA */
		qcom,vin-sel = <2>;		/* Logical 1 voltage value 1.8v */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};

	gpio@c700 { /* GPIO 8 */
		/* USD_DETECT_N */
		qcom,mode = <0>;                /* Digital in */
		qcom,pull = <0>;                /* PULL up 30uA */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,invert = <1>;		/* Output high */
		qcom,vin-sel = <2>;             /* Logical 1 voltage value 1.8v */
		qcom,src-sel = <0>;             /* Constant */
		qcom,out-strength = <1>;	/* Low drive strength */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};

	gpio@c800 { /* GPIO 9 */
		/* DISCRETE_3V5_BOOST_BYPASS_EN */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,output-type = <0>;         /* QPNP_PIN_OUT_BUF_CMOS */
		qcom,out-strength = <1>;         /* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,vin-sel = <2>;		/* QPNP_PIN_VIN2 */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};

	gpio@cb00 { /* GPIO 12 */
		/* PM8994_GPIO12_TPAD_1V8_EN */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,output-type = <0>;          /* QPNP_PIN_OUT_BUF_CMOS */
		qcom,out-strength = <1>;         /* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,vin-sel = <2>;		/* QPNP_PIN_VIN2 */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};

	gpio@cc00 { /* GPIO 13 */
		/* PM8994_GPIO13_TPAD_2V85_EN */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,output-type = <0>;          /* QPNP_PIN_OUT_BUF_CMOS */
		qcom,out-strength = <1>;         /* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,vin-sel = <2>;		/* QPNP_PIN_VIN2 */
		qcom,src-sel = <0>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};

	gpio@cd00 { /* GPIO 14 */
		/* PM8994_GPIO14_DISP_1V8_EN */
		qcom,mode = <1>;		/* QPNP_PIN_MODE_DIG_OUT */
		qcom,pull = <5>;		/* QPNP_PIN_PULL_NO */
		qcom,output-type = <0>;          /* QPNP_PIN_OUT_BUF_CMOS */
		qcom,out-strength = <3>;         /* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,vin-sel = <2>;		/* QPNP_PIN_VIN2 */
		qcom,src-sel = <1>;		/* Constant */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};


	gpio@ce00 { /* GPIO 15 */
		/* DIVCLK1_CODEC */
		qcom,mode = <1>;
		qcom,output-type = <0>;
		qcom,pull = <5>;
		qcom,vin-sel = <2>;
		qcom,out-strength = <1>;
		qcom,src-sel = <2>;
		qcom,master-en = <1>;
		status = "okay";
	};

	gpio@d100 { /* GPIO 18 */
		qcom,mode = <1>;		/* Digital output */
		qcom,output-type = <0>;		/* CMOS logic */
		qcom,invert = <0>;		/* Output low initially */
		qcom,vin-sel = <2>;		/* VIN 2 */
		qcom,src-sel = <3>;		/* Function 2 */
		qcom,out-strength = <2>;	/* Medium */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};

	gpio@d200 { /* GPIO 19 */
		/* KEYPAD_RST_N */
		qcom,mode = <1>;		/* Digital output*/
		qcom,output-type = <0>;          /* QPNP_PIN_OUT_BUF_CMOS */
		qcom,out-strength = <1>;         /* QPNP_PIN_OUT_STRENGTH_LOW */
		qcom,pull = <5>;		/* no pull */
		qcom,vin-sel = <2>;		/* VIN2 */
		qcom,src-sel = <0>;		/* GPIO */
		qcom,invert = <0>;		/* Invert */
		qcom,master-en = <1>;		/* Enable GPIO */
		status = "okay";
	};
};

&pm8994_mpps {
