#include "msm8992.dtsi"
#include "msm8992-pinctrl.dtsi"
#include "dsi-panel-logan-wqhd-dualmipi0-cmd.dtsi"
#include "dsi-panel-logan-wqhd-dualmipi1-cmd.dtsi"
#include "msm8992-camera-sensor-venice.dtsi"

/ {
	aliases {
		i2c1 = &i2c_1;
		i2c5 = &i2c_5;
		i2c7 = &i2c_7;
		i2c10 = &i2c_10;
		i2c11 = &i2c_11;
		i2c12 = &i2c_12;
		uart8 = &blsp2_uart2;
	};
	hwfeatures {
		/*
		 * Common NFC properties for all platforms
		 * Platform dependent properties with same property names override
		 * common properties
		 */
		nfc {
			devnode = "/dev/bcm2079x";
			hal_module_ids = "nfc_nci.bcm2079x-brcm,nfc_nci.bcm2079x";
			lib_module_ids = "libnfc-nci-brcm,libnfc-nci";
			clk_frequency = "26000";
			config_base = "libnfc-brcm";
		};
	};

	memory {
		reset_mem: reset_mem@0 {
			linux,reserve-region;
			linux,remove-completely;
			linux,reserve-contiguous-region;
			/*
			 * Bootloader take the address from the oem,size props
			 * and insert it here along with the address
			 */
			reg = <0 0 0 0>;
			oem,size = <0x1000>;
			oem,writeable;
			label = "reset";
		};

		reset_log_mem: reset_log_mem@0 {
			linux,reserve-region;
			linux,remove-completely;
			linux,reserve-contiguous-region;
			/*
			 * Bootloader take the address from the oem,size props
			 * and insert it here along with the address
			 */
			reg = <0 0 0 0>;
			oem,size = <0x4000>;
			/* If ddt is enabled, we reserve more memory */
			oem,size-ddt = <0x80000>;
			label = "reset_log";
		};

                /* 
                 * increasing to 110 MB (0x6E00000) of physical pool
                 */
                modem_mem: modem_region@0 {
                        reg = <0 0x09000000 0 0x6E00000>;
                };
	};

	oem,preserved_reset_region {
		compatible = "oem,preserved_reset_region";
		regions = <&reset_mem &reset_log_mem> ;
	};

	oem,export_bsi {
		compatible = "oem,export_bsi_region";
	};

	board_mac_addr {
		compatible = "oem,board_mac_addr";
		macaddresswlan = [ 00 00 00 00 00 00 ]; /* Filled by bootchain */
	};

};

/delete-node/ &i2c_5;

/* BB changes to msm8992-pinctrl.dtsi for venice platform */
&tlmm_pinmux {
	tscrn-int {
		qcom,pins = <&gp 64>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "tscrn-int";

		tscrn_int_default: tscrn_int_default {
			bias-pull-up;
		};
	};

	/* KEYPAD INTERRUPT */
	keypad-int {
		qcom,pins = <&gp 9>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "keypad-int";

		keypad_int_default: keypad_int_default {
			bias-pull-up;
		};
	};

	tkpad-int {
		qcom,pins = <&gp 102>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "tkpad-int";

		tkpad_int_default: tkpad_int_default {
			bias-pull-up;
		};
	};

	pmx_mdss_te {
		qcom,pins = <&gp 10>;
		qcom,num-grp-pins = <1>;
	};

	/* I2C_TOUCHPAD */
	pmx_i2c_1 {
		qcom,pins = <&gp 2>, <&gp 3>; /* SDA, SCL */
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <3>;
		label = "pmx_i2c_1";

		status = "ok";

		i2c_1_active: i2c_1_active {
			drive-strength = <2>;
			bias-disable;
		};

		i2c_1_sleep: i2c_1_sleep {
			drive-strength = <2>;
			bias-disable;
		};
	};

	/* I2C_TSCRN uses pmx_i2c_2. Already defined in pinctrl */

	/* I2C_CAM_FF and I2C_CAM are handled via CCI, not regular i2c */

	/* I2C_HUB */
	pmx_i2c_5_1 {
		qcom,pins = <&gp 23>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <4>;
		label = "pmx_i2c_5_1";
		i2c_5_1_active: i2c_5_1_active {
			drive-strength = <2>;
			bias-disable;
		};

		i2c_5_1_sleep: i2c_5_1_sleep {
			drive-strength = <2>;
			bias-disable;
		};
	};

    /*Change the default drive strength values*/
    /*from the default QC configuration*/
	pmx_sdc2_clk {
		sdc2_clk_on: clk_on {
			bias-disable; /* NO pull */
			drive-strength = <16>; /* 16 MA */
		};
	};

	pmx_sdc2_cmd {
		sdc2_cmd_on: cmd_on {
			bias-pull-up; /* pull up */
			drive-strength = <6>; /* 6 MA */
		};
	};

	pmx_sdc2_data {
		sdc2_data_on: data_on {
			bias-pull-up; /* pull up */
			drive-strength = <6>; /* 6 MA */
		};
	};

	/* For whatever reason, QC changed the function for GP24 */
	pmx_i2c_5_2 {
		qcom,pins = <&gp 24>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <5>;
		label = "pmx_i2c_5_2";
		i2c_5_2_active: i2c_5_2_active {
			drive-strength = <2>;
			bias-disable;
		};

		i2c_5_2_sleep: i2c_5_2_sleep {
			drive-strength = <2>;
			bias-disable;
		};
	};
	/* I2C_NFC_FLASH */
	pmx_i2c_6 {
		qcom,pins = <&gp 27>, <&gp 28>;
		qcom,pin-func = <3>;
	};

	/* I2C_WC_ALS */
	pmx_i2c_7 {
		qcom,pins = <&gp 43>, <&gp 44>; /* SDA, SCL */
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <3>;
		label = "pmx_i2c_7";

		i2c_7_active: i2c_7_active {
			drive-strength = <2>;
			bias-disable;
		};

		i2c_7_sleep: i2c_7_sleep {
			drive-strength = <2>;
			bias-disable;
		};
	};

	/* I2C_ANX */
	pmx_i2c_10 {
		qcom,pins = <&gp 55>, <&gp 56>; /* SDA, SCL */
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <4>;
		label = "pmx_i2c_10";

		i2c_10_active: i2c_10_active {
			drive-strength = <2>;
			bias-disable;
		};

		i2c_10_sleep: i2c_10_sleep {
			drive-strength = <2>;
			bias-disable;
		};
	};

	/* I2C_DDR_PWR */
	pmx_i2c_11 {
		qcom,pins = <&gp 83>, <&gp 84>; /* SDA, SCL */
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <3>;
		label = "pmx_i2c_11";

		i2c_11_active: i2c_11_active {
			drive-strength = <2>;
			bias-disable;
		};

		i2c_11_sleep: i2c_11_sleep {
			drive-strength = <2>;
			bias-disable;
		};
	};

	/* I2C_KEYPAD */
	pmx_i2c_12 {
		qcom,pins = <&gp 87>, <&gp 88>; /* SDA, SCL */
		qcom,num-grp-pins = <2>;
		qcom,pin-func = <3>;
		label = "pmx_i2c_12";

		i2c_12_active: i2c_12_active {
			drive-strength = <2>;
			bias-disable;
		};

		i2c_12_sleep: i2c_12_sleep {
			drive-strength = <2>;
			bias-disable;
		};
	};

	cam_sensor_rear_active {
		/* RESET */
		qcom,pins = <&gp 92>;
		qcom,num-grp-pins = <1>;
	};

	cam_sensor_rear_suspend {
		/* RESET */
		qcom,pins = <&gp 92>;
		qcom,num-grp-pins = <1>;
	};

	cam_sensor_front_active {
		/* RESET */
		qcom,pins = <&gp 104>;
		qcom,num-grp-pins = <1>;
	};

	cam_sensor_front_suspend {
		/* RESET */
		qcom,pins = <&gp 104>;
		qcom,num-grp-pins = <1>;
	};

	/* MSM_GPIO_40 */
	pmx_bt_host_wake {
		qcom,pins = <&gp 40>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "pmx_host_wake";
		host_wake_no_pull: host_wake_no_pull {
			drive-strength = <2>;
			bias-disable;
		};
		host_wake_pull_down: host_wake_pull_down {
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	pmx_blsp2_uart2_sleep {
		qcom,pins = <&gp 45>, <&gp 46>, <&gp 47>, <&gp 48>;
		qcom,num-grp-pins = <4>;
		qcom,pin-func = <0>;
		label = "pmx_blsp2_uart2_sleep";
		blsp2_uart2_sleep: sleep {
			drive-strength = <2>;
			bias-disable;
			input-enable;
		};
	};

	/* NFC INTERRUPT */
	nfc_int {
		qcom,pins = <&gp 29>;
		qcom,pin-func = <0>;
		qcom,num-grp-pins = <1>;
		label = "nfc_int";

		nfc_int_default: nfc_int_default {
			bias-pull-down;
		};
	};

	/* Assign HDMI gpios */
	pmx_hdmi_cec {
		qcom,pins = <&gp 31>;
		qcom,num-grp-pins = <1>;
	};

	pmx_hdmi_ddc {
		qcom,pins = <&gp 32>, <&gp 33>;
		qcom,num-grp-pins = <2>;
	};

	pmx_hdmi_hpd {
		qcom,pins = <&gp 34>;
		qcom,num-grp-pins = <1>;
	};

	pcie0_wake {
		qcom,pins = <&gp 37>;
		qcom,num-grp-pins = <1>;
		label = "pcie0-wake";
		/* default state */
		pcie0_wake_default: pcie0_wake_default {
			drive-strength = <2>;
			/delete-property/ bias-pull-down;
			bias-pull-up;
		};

		pcie0_wake_sleep: pcie0_wake_sleep {
			drive-strength = <2>;
			/delete-property/ bias-disable;
			bias-pull-up;
		};
	};

	/* Override default pull for GPIO_134 (wifi int gpio_4) */
	pmx_lte_coex_tx {
		qcom,pins = <&gp 134>;
		qcom,num-grp-pins = <1>;
		qcom,pin-func = <0>;
		label = "lte_coex_tx_default";
		lte_coex_tx_default:lte_coex_tx_default {
			drive-strength = <2>;
			bias-pull-down;
		};
	};

	/delete-node/ cnss_pins;
};

&soc {
	oem,mpm2-g-rd-cntr@fc4a2000 {
		compatible = "oem,mpm2-g-rd-cntr";
		reg = <0xfc4a2000 0x1000>;
		clock-frequency = <19200000>;
	};

	spi_0: spi@f9923000 {
		status = "disabled";
	};

	/* I2C note: the documentation labels QUPs from 0-5, while the QC-provided DTSIs name them 1-6 */
	i2c_1: i2c@f9923000 { /* BLSP1 QUP1 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr", "bam_phys_addr";
		reg = <0xf9923000 0x1000>,
			<0xf9904000 0x19000>;
		interrupt-names = "qup_irq", "bam_irq";
		interrupts = <0 95 0>, <0 238 0>;
		qcom,clk-freq-out = <400000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks = <&clock_gcc clk_gcc_blsp1_ahb_clk>,
			<&clock_gcc clk_gcc_blsp1_qup1_i2c_apps_clk>;

		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_1_active>;
		pinctrl-1 = <&i2c_1_sleep>;
		qcom,noise-rjct-scl = <0>;
		qcom,noise-rjct-sda = <0>;
		qcom,bam-pipe-idx-cons = <10>;
		qcom,bam-pipe-idx-prod = <11>;
		qcom,master-id = <86>;
		qcom,disable-dma;
		qcom,high-time-clk-div = <21>;
		qcom,fs-clk-div = <21>;

		synaptics_dsx_i2c_keypad@20 {
			compatible = "synaptics,dsx";
			reg = <0x20>;
			synaptics,reg-en;
			interrupt-parent = <&msm_gpio>;
			interrupts = <102 0x2008>;
			vcc-supply = <&pm8994_l18>;
			synaptics,reset-gpio = <&msm_gpio 105 0x00>;
			synaptics,irq-gpio = <&msm_gpio 102 0x00>;
			synaptics,power-gpio = <&pm8994_gpios 12 0x3>;
			synaptics,main-power-gpio = <&pm8994_gpios 13 0x3>;
			synaptics,power-on-status = <1>;
			synaptics,main-power-on-status = <1>;
			synaptics,report-type = <3>;
			synaptics,blacklist-product-id = "s3501b";
			synaptics,disable-baseline-compensation-in-BIST = <0>;
			pinctrl-names = "default";
			pinctrl-0 = <&tkpad_int_default>;
			synaptics,x-flip = <0>;
			synaptics,y-flip = <0>;
			synaptics,reset-on-status = <0>;
			synaptics,power-delay-ms = <1>;
			synaptics,reset-delay-ms = <63>;
			synaptics,reset-active-ms = <15>;
			synaptics,watchdog-timeout-s = <10>;
			synaptics,bist-min = <1980>;
			synaptics,bist-max = <6000>;
			synaptics,input-device-name = "touch_keypad";
			synaptics,resolution_x = <21>;
			synaptics,resolution_y = <21>;
		};
	};

	/* i2c_2 already defined in msm8992.dtsi  (BLSP1 QUP2)*/
	i2c_2: i2c@f9924000 {
		qcom,clk-freq-out = <400000>;
		qcom,high-time-clk-div = <21>;
		qcom,fs-clk-div = <21>;
		synaptics_dsx_i2c_display@67 {
			compatible = "synaptics,dsx";
			reg = <0x67>;
			synaptics,reg-en;
			interrupt-parent = <&msm_gpio>;
			interrupts = <64 0x2008>;
			vcc-supply = <&pm8994_l22>;
			vcc_i2c-supply = <&pm8994_lvs1>;
			synaptics,i2c-pull-up;
			synaptics,reset-gpio = <&msm_gpio 109 0x00>;
			synaptics,irq-gpio = <&msm_gpio 64 0x00>;
			pinctrl-names = "default";
			pinctrl-0 = <&tscrn_int_default>;
			synaptics,x-flip = <0>;
			synaptics,y-flip = <0>;
			synaptics,reset-on-status = <0>;
			synaptics,power-delay-ms = <1>;
			synaptics,reset-delay-ms = <63>;
			synaptics,reset-active-ms = <15>;
			synaptics,watchdog-timeout-s = <10>;
			synaptics,bist-min = <500>;
			synaptics,bist-max = <1450>;
			synaptics,input-device-name = "touch_display";
			synaptics,resolution_x = <21>;
			synaptics,resolution_y = <21>;
			synaptics,wakeup-gesture;
			synaptics,wakeup-gesture-no-criteria;
		};

		synaptics_dsx_i2c_display@20 {
			compatible = "synaptics,dsx";
			reg = <0x20>;
			synaptics,reg-en;
			interrupt-parent = <&msm_gpio>;
			interrupts = <64 0x2008>;
			vcc-supply = <&pm8994_l22>;
			vcc_i2c-supply = <&pm8994_lvs1>;
			synaptics,i2c-pull-up;
			synaptics,reset-gpio = <&msm_gpio 109 0x00>;
			synaptics,irq-gpio = <&msm_gpio 64 0x00>;
			pinctrl-names = "default";
			pinctrl-0 = <&tscrn_int_default>;
			synaptics,x-flip = <0>;
			synaptics,y-flip = <0>;
			synaptics,reset-on-status = <0>;
			synaptics,power-delay-ms = <1>;
			synaptics,reset-delay-ms = <63>;
			synaptics,reset-active-ms = <15>;
			synaptics,watchdog-timeout-s = <10>;
			synaptics,bist-min = <500>;
			synaptics,bist-max = <1450>;
			synaptics,input-device-name = "touch_display";
			synaptics,resolution_x = <21>;
			synaptics,resolution_y = <21>;
		};
	};

	/* No i2c_3 or i2c_4. i2c for camera is handled via CCI not regular i2c driver */

	i2c_5: i2c@f9927000 { /* BLSP1 QUP5 */
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr";
		reg =	<0xf9927000 0x1000>;
		interrupt-names = "qup_irq";
		interrupts = <0 99 0>;
		qcom,clk-freq-out = <1000000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks =<&clock_gcc clk_gcc_blsp1_ahb_clk>,
			<&clock_gcc clk_gcc_blsp1_qup5_i2c_apps_clk>;

		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_5_1_active &i2c_5_2_active>;
		pinctrl-1 = <&i2c_5_1_sleep &i2c_5_2_sleep>;
		qcom,noise-rjct-scl = <0>;
		qcom,noise-rjct-sda = <0>;
		dmas = <&dma_blsp1 20 64 0x20000020 0x20>,
			<&dma_blsp1 21 32 0x20000020 0x20>;
		dma-names = "tx", "rx";
		qcom,master-id = <86>;
		m4_hub@39 {
			compatible = "stm,m4_hub";
			reg = <0x39>;
			vcc-supply = <&pm8994_s4>;
			vcc_i2c-supply = <&pm8994_lvs2>;
			vcc_sensors-supply = <&pm8994_l18>;
			userspace = <1>;
			hub_boot0 = <&msm_gpio 110 0x00>;
			hub_boot1 = <&msm_gpio 107 0x00>;
			hub_rst   = <&msm_gpio  63 0x00>;
			msm_sleep = <&msm_gpio  38 0x00>;
			hub_int   = <&msm_gpio 108 0x00>;
			msm_int_hub_wake_n = <&msm_gpio 113 0x00>;
		};
	};

	i2c_6: i2c@f9928000 { /* BLSP1 QUP6 */
		status = "ok";
		qcom,disable-dma;
		bcm_nfc_i2c@76 {
			compatible = "bcm,bcm2079x";
			/* slave address */
			reg = <0x76>;
			/* power */
			bcm,nfc-vddio-supply = <&pm8994_l30>;
			/* reset */
			bcm,ena-gpio = <&msm_gpio 30 0x00>;
			/* wake */
			bcm,wak-gpio = <&msm_gpio 12 0x01>;
			/* clock - individual crystal 26MHz clock */
			/* bcm,clk-req-gpio = <&pm8994_gpios 10 0x00>; */
			/* bcm,clk-src = "BBCLK2"; */
			/* clocks = <&clock_rpm clk_bb_clk2_pin>; */
			/* clock-names = "ref_clk"; */
			/* interrupt */
			bcm,irq-gpio = <&msm_gpio 29 0x00>;
			interrupt-parent = <&msm_gpio>;
			interrupts = <29 0>;
			interrupt-names = "nfc_irq";
			pinctrl-names = "default";
			pinctrl-0 = <&nfc_int_default>;
		};
	};

	i2c_7: i2c@f9963000 { /* BLSP2 QUP1 */
		status = "ok";
		compatible = "qcom,i2c-msm-v2";
		#address-cells = <1>;
		#size-cells = <0>;
		reg-names = "qup_phys_addr", "bam_phys_addr";
		reg =	<0xf9963000 0x1000>,
			<0xf9944000 0x19000>;
		interrupt-names = "qup_irq", "bam_irq";
		interrupts = <0 101 0>, <0 239 0>;
		qcom,clk-freq-out = <400000>;
		qcom,clk-freq-in  = <19200000>;
		clock-names = "iface_clk", "core_clk";
		clocks =<&clock_gcc clk_gcc_blsp2_ahb_clk>,
			<&clock_gcc clk_gcc_blsp2_qup1_i2c_apps_clk>;

		pinctrl-names = "i2c_active", "i2c_sleep";
		pinctrl-0 = <&i2c_7_active>;
		pinctrl-1 = <&i2c_7_sleep>;
		qcom,noise-rjct-scl = <0>;
		qcom,noise-rjct-sda = <0>;
		qcom,bam-pipe-idx-cons = <12>;
		qcom,bam-pipe-idx-prod = <13>;
		qcom,master-id = <84>;
		qcom,disable-dma;
		stmvl6180@29 {
			compatible = "stm,stmvl6180";
			vcc-supply = <&pm8994_l18>;
			vcc_i2c-supply = <&pm8994_lvs2>;
			reg = <0x29>;
			interrupt  = <&msm_gpio 73 0x00>;
			reset  = <&msm_gpio 62 0x00>;
			userspace = <1>;
		};

		/* IDT-P9028 wireless power IC */
		idt-p9028@25 {
