&blsp1_uart2 {
	status= "ok";
	qcom,use-pm;
	qcom,config-gpio = <2>;
	qcom,tx-gpio = <&msm_gpio 4 0x00>;
	qcom,rx-gpio = <&msm_gpio 5 0x00>;
	pinctrl-names = "default";
	pinctrl-0 = <&uart_console_sleep>;
};

&blsp2_uart2 { /* BT UART */
	/delete-property/ #address-cells;
	/delete-property/ interrupt-parent;
	/delete-property/ #interrupt-cells;
	/delete-property/ interrupt-map-mask;
	/delete-property/ interrupt-map;
	interrupts = <0 114 0>, <0 239 0>;
	interrupt-names = "core_irq", "bam_irq";
	qcom,tx-gpio = <&msm_gpio 45 0x00>;
	qcom,rx-gpio = <&msm_gpio 46 0x00>;
	qcom,cts-gpio = <&msm_gpio 47 0x00>;
	qcom,rfr-gpio = <&msm_gpio 48 0x00>;
	qcom,msm-obs;
	pinctrl-names = "sleep", "default";
	pinctrl-0 = <&blsp2_uart2_sleep>;
	pinctrl-1 = <&hsuart_active>;
	status = "ok";
};

