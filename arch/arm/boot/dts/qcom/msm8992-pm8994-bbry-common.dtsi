	   skipped in qpnp-power-on.c */
	qcom,pm8994@0 {
		qcom,power-on@800 {
			qcom,store-hard-reset-reason;
			qcom,s3-debounce = <16>;
			qcom,s3-src = "kpdpwr";

			qcom,pon_1 {
				qcom,s2-type = <8>; /* DVDD_HARD_RESET */
				qcom,s2-timer = <1000>; /* 10+1=11 secs */
			};

			qcom,pon_2 {
				linux,code = <183>;
				qcom,support-reset = <1>;
				qcom,s1-timer = <6720>;
				qcom,s2-timer = <2000>;
				qcom,s2-type = <1>;
			};

			qcom,pon_3 {
				qcom,support-reset = <0>;
			};
		};
	};

	qcom,pmi8994@2 {
		qcom,power-on@800 {
			qcom,s3-debounce = <16>;
			qcom,s3-src = "kpdpwr";
		};
	};
};

